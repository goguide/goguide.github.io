{
  "name": "goguide.github.io",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "watch": "set WATCH=true && chokidar \"src/*.*\" -c \"npm run build\"",
    "serve": "browser-sync start --files \"dist\" --server \"dist\"",
    "start": "npm-run-all --parallel serve watch",
    "i18n": "node i18n-gen.js",
    "deploy": "gh-pages -d dist -b master",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build && npm run i18n"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "browser-sync": "^2.26.7",
    "chokidar-cli": "^1.2.2",
    "gh-pages": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.14.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-copy": "^2.0.1",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-uglify": "^6.0.2"
  }
}
