{
  "name": "goguide.github.io",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "serve": "browser-sync start --files \"dist\" --server \"dist\"",
    "i18n": "node i18n-gen.js",
    "deploy": "gh-pages -d dist -b master",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build && npm run i18n"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "gh-pages": "^2.0.1",
    "rollup": "^1.14.4",
    "rollup-plugin-copy": "^2.0.1"
  }
}
